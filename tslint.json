{
  "extends": "tslint-microsoft-contrib/recommended_ruleset",
  "rules": {
    "interface-name": false, // c.f. https://github.com/Microsoft/TypeScript/wiki/Coding-guidelines#names
    "missing-jsdoc": false,
    "no-default-export": false, // required to use react
    "no-use-before-declare": false // due to https://github.com/palantir/tslint/issues/1400
  },
  "jsRules": {
  },
  "rulesDirectory": [
    "node_modules/tslint-microsoft-contrib"
  ]
}
