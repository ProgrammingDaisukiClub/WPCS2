<p id="notice"><%= notice %></p>

<h1>Data Sets</h1>

<table>
  <thead>
    <tr>
      <th>id</th>
      <th>problem_id</th>
      <th>label</th>
      <th>order</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @data_sets.each do |data_set| %>
      <tr>
        <td><%= data_set.id %></td>
        <td><%= data_set.problem_id %></td>
        <td><%= data_set.label %></td>
        <td><%= data_set.order %></td>
        <td><%= link_to 'Show', [:admin, data_set] %></td>
        <td><%= link_to 'Edit', edit_admin_data_set_path(data_set) %></td>
        <td><%= link_to 'Destroy', [:admin, data_set], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Data Set', new_admin_data_set_path %>
